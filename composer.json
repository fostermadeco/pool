{
    "name": "fostermadeco/pool",
    "description": "A Segment package for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Winfree",
            "email": "adam.winfree@fostermade.co"
        }
    ],
    "require": {
        "illuminate/config": "~5.6.0",
        "illuminate/database": "~5.6.0",
        "illuminate/support": "~5.6.0",
        "segmentio/analytics-php": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "FosterMadeCo\\Pool\\": "src/Pool"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0",
        "phpunit/phpunit": "^7.2",
        "illuminate/auth": "^5.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FosterMadeCo\\Pool\\PoolServiceProvider"
            ],
            "aliases": {
                "Analytics": "FosterMadeCo\\Pool\\Facades\\Analytics"
            }
        }
    }
}
